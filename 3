#include<iostream>
#include<cmath>
#include<string>
using namespace std;
int main()
{
	setlocale(0, "");
	int k = 1;
	cout << "Ââåäèòå êîëè÷åñòâî\n";
	cin >> k;
        string apple = "ÿáëîê";
		if (k == 1 || k % 10)
			apple += "î";
		if ((k > 1 && k < 5) || (k % 10 > 1 && k % 10 < 5))
			apple += "à";
		if ((k >= 5 && k < 10) || (k % 10 >= 5 && k % 10 < 10) || (k % 100 > 10 && k % 100 < 15))
			apple = "ÿáëîê";
		cout << k << apple << '\n';
return 0;
}
